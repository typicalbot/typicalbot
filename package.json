{
    "name": "typicalbot",
    "version": "3.0.1",
    "description": "TypicalBot is open-source discord bot written in JavaScript, using the Discord.JS library.",
    "license": "Apache-2.0",
    "main": "index.js",
    "scripts": {
        "build": "tsc --watch",
        "build:database": "tsc && node production/scripts/setup.js",
        "build:instance": "tsc && node production/scripts/pm2.js",
        "lint": "tsc --noEmit && eslint . --ext .ts'",
        "lint:fix": "tsc --noEmit && eslint . --ext .ts --fix",
        "start": "tsc && node production/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typicalbot/typicalbot.git"
    },
    "keywords": [
        "discordjs",
        "javascript",
        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/typicalbot/typicalbot/issues"
    },
    "homepage": "https://github.com/typicalbot/typicalbot#readme",
    "dependencies": {
        "@typicalbot/eslint-config-typicalbot": "github:typicalbot/eslint-config-typicalbot",
        "canvas": "^2.6.0",
        "canvas-constructor": "^3.1.0",
        "discord.js": "github:discordjs/discord.js#master",
        "fs": "0.0.2",
        "i18next": "^19.1.0",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^6.2.1",
        "moment": "^2.22.2",
        "node-fetch": "^2.6.0",
        "pm2": "^4.2.3",
        "rethinkdb-ts": "^2.4.2",
        "simple-youtube-api": "github:hypercoder2975/simple-youtube-api",
        "simple-youtube-stream": "github:hypercoder2975/simple-youtube-stream",
        "typescript": "^3.7.4",
        "veza": "^1.1.0",
        "vm2": "^3.8.3",
        "ytdl-core": "github:fent/node-ytdl-core"
    },
    "optionalDependencies": {
        "bignum": "^0.13.0",
        "bufferutil": "^4.0.1",
        "erlpack": "github:discordapp/erlpack",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.5"
    },
    "devDependencies": {
        "@types/i18next-node-fs-backend": "^2.1.0",
        "@types/klaw": "^3.0.0",
        "@types/mathjs": "^6.0.4",
        "@types/node": "^12.12.26",
        "@types/node-fetch": "^2.5.1",
        "@types/ws": "^7.2.1",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "eslint": "^6.1.0",
        "eslint-config-typicalbot": "github:typicalbot/eslint-config-typicalbot",
        "eslint-plugin-import": "^2.20.1"
    },
    "eslintConfig": {
        "extends": "eslint-config-typicalbot"
    }
}
