{
    "name": "typicalbot",
    "version": "3.6.0",
    "description": "TypicalBot is far from typical. It's stable, lightning fast, and easy to useâ€” TypicalBot will seamlessly help you moderate your server and provide entertainment for its members, no problem at all!",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "tsc",
        "build:clean": "git clean -d -x -e node_modules -e etc/config.json -f && tsc",
        "build:watch": "tsc --watch",
        "build:database": "tsc && node dist/bin/setup.js",
        "build:instance": "tsc && node dist/bin/pm2.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "tsc && node dist/src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sylkellc/typicalbot.git"
    },
    "keywords": [
        "discordjs",
        "javascript",
        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/sylkellc/typicalbot/issues"
    },
    "homepage": "https://github.com/sylkellc/typicalbot#readme",
    "dependencies": {
        "@sentry/node": "^5.15.5",
        "discord.js": "^12.2.0",
        "fs": "0.0.2",
        "i18next": "^19.4.4",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^7.0.0",
        "moment": "^2.25.3",
        "node-fetch": "^2.6.0",
        "pm2": "^4.4.0",
        "rethinkdb-ts": "^2.4.2",
        "typescript": "^3.8.3",
        "veza": "^1.1.0",
        "vm2": "^3.9.2",
        "winston": "^3.2.1"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.1",
        "erlpack": "github:discord/erlpack",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/i18next-node-fs-backend": "^2.1.0",
        "@types/klaw": "^3.0.1",
        "@types/mathjs": "^6.0.5",
        "@types/node": "^12.12.29",
        "@types/node-fetch": "^2.5.7",
        "@types/ws": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.33.0",
        "eslint": "^6.1.0",
        "eslint-plugin-import": "^2.20.2",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
